<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>KdSoft.Services.Protobuf Class Library</Description>
    <Copyright>Karl Waclawek</Copyright>
    <AssemblyTitle>KdSoft.Services.Protobuf</AssemblyTitle>
    <Authors>Karl Waclawek</Authors>
    <TargetFramework>netstandard1.1</TargetFramework>
    <AssemblyName>KdSoft.Services.Protobuf</AssemblyName>
    <PackageId>KdSoft.Services.Protobuf</PackageId>
    <GenerateAssemblyTitleAttribute>false</GenerateAssemblyTitleAttribute>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <GenerateAssemblyCopyrightAttribute>false</GenerateAssemblyCopyrightAttribute>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\..\SharedAssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\KdSoft.Data.Models.Shared\KdSoft.Data.Models.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.2.0" />
    <PackageReference Include="KdSoft.Reflection.Portable" Version="1.0.3" />
  </ItemGroup>

</Project>
